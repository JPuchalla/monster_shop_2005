<h1>Edit Item</h1>

<%= form_for [:merchant, @item] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name%>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <%= f.label :price %>
  <%= f.text_field :price%>
  <%= f.label :inventory, 'Current Inventory Count' %>
  <%= f.text_field :inventory%>
  <%= f.submit %>
<% end %>
